{
  "name": "MessageServer",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "watch": "webpack --watch",
    "dev": "webpack-dev-server --env.dev",
    "build": "webpack --config webpack.server.config",
    "build:server": "BABEL_ENV=production webpack --config=config/webpack.server.js --env.NODE_ENV=production",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "NODE_ENV=dev nodemon server.js --exec babel-node"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "es6": true,
      "browser": true
    },
    "rules": {
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "comma-dangle": "off",
      "no-unused-vars": "warn",
      "no-var": "off",
      "one-var": "off",
      "no-console": "off",
      "react/jsx-filename-extension": "off"
    }
  },
  "dependencies": {
    "@babel/polyfill": "7.2.5",
    "@babel/runtime": "7.3.4",
    "bcrypt": "3.0.4",
    "cookie-parser": "1.4.4",
    "express": "4.16.4",
    "express-session": "1.15.6",
    "graphql": "14.1.1",
    "mongoose": "5.4.15",
    "morgan": "1.9.1",
    "passport": "0.4.0",
    "passport-auth0": "1.1.0"
